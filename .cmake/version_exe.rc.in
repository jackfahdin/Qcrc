1 VERSIONINFO
 FILEVERSION ${GIT_TAG_WITH_COMMA},${COMMIT_COUNTS}
 PRODUCTVERSION ${GIT_TAG_WITH_COMMA},${COMMIT_COUNTS}
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x40004L
 FILETYPE 0x2L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "080404b0"
        BEGIN
            VALUE "CompanyName", "Jackfahdin"
            VALUE "FileDescription", "${PROJECT_DESCRIPTION}"
            VALUE "FileVersion", "${LATEST_TAG}.${COMMIT_NEW_TAG_COUNTS}"
            VALUE "InternalName", "${PROJECT_NAME}.exe"
            VALUE "LegalCopyright", "Copyright (C) 2024 Jackfahdin. All rights reserved."
            VALUE "OriginalFilename", "${PROJECT_NAME}.exe"
            VALUE "ProductName", "${PROJECT_NAME}"
            VALUE "ProductVersion", "${LATEST_TAG}"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x804, 1200
    END
END

IDI_ICON1 ICON DISCARDABLE "${CMAKE_SOURCE_DIR}/Image/AppIcon.ico"
